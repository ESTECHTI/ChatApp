{
  "expo": {
    "name": "TellMe",
    "description": "This project is really great.",
    "slug": "TellMe",
    "privacy": "public",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/mipmap-xhdpi/ic_launcher.png",
    "splash": {
      "image": "./assets/splash/mipmap-xxxhdpi/ic_launcherSplash.png",
      "resizeMode": "contain",
